{
  "name": "blankwhitecards",
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "host": "tsc -b && vite build --mode development; vite preview --host",
    "lint": "eslint .",
    "preview": "vite preview",
    "pull:global": "curl https://white.mcteamster.com/decks/global.json > ./public/decks/global.json",
    "serve": "node --env-file=.env.production -r ts-node/register ./src/server.ts",
    "serve:local": "node --env-file=.env.development -r ts-node/register ./src/server.ts",
    "deploy": "../.ci/deploy-white-service.sh; ../.ci/deploy-white.sh",
    "deploy:client": "../.ci/deploy-white.sh",
    "deploy:service": "../.ci/deploy-white-service.sh"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.0",
    "@microsoft/clarity": "^1.0.0",
    "@mui/icons-material": "^6.4.5",
    "atrament": "^4.6.0",
    "boardgame.io": "^0.50.2",
    "nanoid": "^5.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-qr-code": "^2.0.15",
    "react-router": "^7.2.0",
    "wired-elements": "^3.0.0-rc.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.2.0"
  }
}
